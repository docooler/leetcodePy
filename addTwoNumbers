class Solution:
    # @return a ListNode
    def addTwoNumbers(self, l1, l2):
        head = l1
        prev = l1
        carrier = 0
        while l1 != None:
            addSum = l1.val + l2.val + carrier
            val = addSum%10
            l1.val = val
            carrier = addSum/10
            prev = l1
            l1 = l1.next
            l2 = l2.next
        if carrier==1:
            prev.next = ListNode(1)
            prev.next.next = None
        return head
